{
  "expo": {
    "name": "ArtistCRM",
    "slug": "artistcrm-mobile",
    "scheme": "artistcrm",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "plugins": ["expo-router", "expo-secure-store", "expo-notifications"],
    "experiments": {
      "typedRoutes": true
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#f7efe1"
      }
    }
  }
}
